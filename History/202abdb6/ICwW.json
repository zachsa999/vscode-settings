{
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "explorer.confirmDelete": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "rust": false,
    "javascript": true,
    "svelte": true,
    "css": true,
    "typescript": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "files.autoSaveDelay": 100,
  "files.associations": {
    // "*.svelte.md": "svelte"
  },
  "explorer.confirmDragAndDrop": false,
  "svelte.enable-ts-plugin": true,
  "editor.foldingMaximumRegions": 50000,
  "terminal.integrated.defaultProfile.linux": "bash",
  "git.confirmSync": false,
  "editor.fontLigatures": "'ss02' on",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.fontFamily": "MonoLisa",
  "editor.lineHeight": 1.8,
  "editor.fontSize": 14,
  "editor.multiCursorModifier": "ctrlCmd",
  "window.menuBarVisibility": "compact",
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": true,
  "workbench.editor.untitled.hint": "hidden",
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.labelFormat": "short",
  "cSpell.userWords": [
    "autoprefixer",
    "Braden",
    "fetchable",
    "hamlindigo",
    "IFTA",
    "imagetools",
    "mailgun",
    "mdsvex",
    "oldschool",
    "pdfs",
    "rehype",
    "skeletonlabs",
    "supabase",
    "sveltejs",
    "sveltekit",
    "Wiebe"
  ],
  "svelte.plugin.svelte.note-new-transformation": false,
  "editor.minimap.enabled": false,
  "terminal.integrated.fontFamily": "MonoLisa",
  "terminal.integrated.lineHeight": 0.7,
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.accessibilitySupport": "off",
  "sundial.autoLocale": true,
  "workbench.preferredDarkColorTheme": "Monokai Pro",
  "sundial.interval": 20,
  "sundial.nightSettings": {},
  "workbench.iconTheme": "Monokai Pro Icons",
  "monkeyC.developerKeyPath": "c:\\Users\\Zach\\Documents\\projects\\watch-garmin\\developer_key",
  "editor.formatOnSave": true,
